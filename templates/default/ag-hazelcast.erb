alfresco.cluster.enabled=<%= node['alfresco.cluster.enabled']%>
alfresco.cluster.name=alfresco
alfresco.hazelcast.tcp.config=<members><% for @item in node["additional_cluster_members"] %>
    <%= @item %>,
  	<% end %><%= node['ipaddress']%></members>
alfresco.hazelcast.specify.interface=true
alfresco.hazelcast.bind.interface=<%= node['ipaddress']%>
alfresco.hazelcast.port=5701
alfresco.hazelcast.autoinc.port=false
